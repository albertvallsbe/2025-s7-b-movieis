{
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"extends": ["eslint:recommended", "prettier"],
	"env": {
		"es6": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"no-console": "warn",
		"@typescript-eslint/no-unused-vars": [
			"error",
			{ "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
		]
	}
}
